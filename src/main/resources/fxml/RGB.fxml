<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" styleClass="label" stylesheets="@../styles/Rgb.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.imageprocessing.controllers.RGBController">
   <children>
      <GridPane fx:id="gridpaneRGB" gridLinesVisible="true" prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="labelRed" text="Label" />
            <Label fx:id="labelGreen" text="Label" GridPane.rowIndex="1" />
            <Label fx:id="labelBlue" text="Label" GridPane.rowIndex="2" />
            <ToggleButton fx:id="buttonRed" mnemonicParsing="false" onMouseClicked="#onClickRed" text="ToggleButton" GridPane.columnIndex="1" />
            <ToggleButton fx:id="buttonGreen" mnemonicParsing="false" onMouseClicked="#onClickGreen" text="ToggleButton" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <ToggleButton fx:id="buttonBlue" mnemonicParsing="false" onMouseClicked="#onClickBlue" text="ToggleButton" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
